{
    "extends": "airbnb",
    "plugins": ["import", "react"],
    "parserOptions": {
       "ecmaVersion": 9,
       "sourceType": "module",
       "ecmaFeatures": {
           "jsx": true
       }
   },
   "rules": {
       "camelcase": "error",
       "semi": ["error", "never"]
   },
   "env": {
    "browser": true,
    "es6": true
   }
}